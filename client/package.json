    "babel-loader": "^8.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^4.0.0",
    "tailwindcss": "^1.6.2",
    "typescript": "^3.7.0-beta"
  },
  "scripts": {
